{
  "name": "graphql-server-firebase-authentication-example",
  "version": "0.0.0",
  "scripts": {
    "dev-play": "npm-run-all --parallel start playground",
    "dev": "npm run start ",
    "start": "nodemon -e ts,graphql -x ts-node -r dotenv/config src/index.ts",
    "playground": "graphql playground",
    "build": "rm -rf dist && graphql codegen && tsc",
    "prisma": "prisma"
  },
  "dependencies": {
    "apollo-server": "^2.0.0",
    "bcryptjs": "2.4.3",
    "firebase-admin": "^5.13.1",
    "graphql": "0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-tag": "2.9.2",
    "graphql-tools": "3.0.5",
    "jsonwebtoken": "8.3.0",
    "prisma-binding": "2.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "dotenv": "5.0.1",
    "graphql-cli": "2.16.5",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "prisma": "1.12.0",
    "ts-node": "6.2.0",
    "typescript": "2.9.2"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  }
}
